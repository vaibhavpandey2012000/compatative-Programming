    "tc=int(input())\n",
    "while tc:\n",
    "    k=int(input())\n",
    "    for i in range(64):\n",
    "        if i==0:\n",
    "            print('O',end=\"\")\n",
    "        elif k-i>=0:\n",
    "            print(\"X\",end=\"\")\n",
    "        else:\n",
    "            print(\".\",end=\"\")\n",
    "        if (i+1)%8==0:\n",
    "            print()\n",
    "        tc-=1"
